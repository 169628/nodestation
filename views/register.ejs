    <div class="wrap">
      <%- include("layout/nav")%>
      <!-- <%- include("layout/message")%> -->
      <div class="register">
        <div class="main effect7">
          <ul>
            <li>
              <label for="name">暱稱</label>
              <input type="text" name="name" id="name" />
            </li>
            <li>
              <label for="mail">信箱</label>
              <input type="email" name="mail" id="mail" />
